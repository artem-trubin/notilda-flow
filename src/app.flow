import material/material;

export {
  makeApp() -> MAppStructure;
}

makeApp() -> MAppStructure {
  mockTextForNotes = [
    "Test note",
    "Second test note",
    "Third super ttest note",
  ];

  content = MContent(
    MLines(
      map(
        mockTextForNotes,
        \text -> MText(text, [])
      )
    )
  );

  MAppStructure([content], []);
}
