import material/material;

export {
  makeApp() -> Material;
}

makeApp() -> Material {
  topBar = MAppBar(MText("This is top bar", []));
  leftNav = MLeftNav(
    MText("This is left nav", []),
    make(true),
    []
  );
  content = MContent(MText("This is content", []));

  MAppStructure([topBar, leftNav, content], []);
}
