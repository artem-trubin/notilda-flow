import material/material;

import app/note/note;
import app/note/note_list;
import app/state/state;
import app/view/view;

export {
  makeViewList(
    state : NotildaState,
  ) -> Material;
}

makeViewList(
  state : NotildaState,
) -> Material {
  MSelect(state.notesB, \notes : [Note] -> {
    makeNoteList(notes);
  });
}
