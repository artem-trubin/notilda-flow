import material/material;

import app/mock_data;
import app/note/note;
import app/note/note_list;

export {
  makeApp() -> MAppStructure;
}

makeApp() -> MAppStructure {
  notesB = make(makeMockNotes());

  notesList = MSelect(notesB, makeNoteList);

  page = MColsA([
    notesList
  ]);

  MAppStructure([MContent(page)], []);
}
