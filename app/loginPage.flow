import material/material2tropic;

import app/state;

export {
  makePageLogin(state : State) -> Material;
}

makePageLogin(state : State) -> Material {
  usernameB = make("");
  usernameInput = MTextInput(usernameB, [MLabel("username"), MFloatingLabel()], []);

  passwordB = make("");
  passwordInput = MTextInput(passwordB, [MLabel("Password"), MFloatingLabel()], []);

  MLines([
    usernameInput,
    passwordInput,
  ]);
}
