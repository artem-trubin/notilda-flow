import material/material2tropic;

import app/note/note;
import app/note/note_card;

export {
  makeNoteList(
    notes : [Note],
    openNote : (note : Note) -> void,
  ) -> Material;
}

makeNoteList(
  notes : [Note],
  openNote : (note : Note) -> void,
) -> Material {

  MLinesA(map(notes, \n -> makeNoteCard(n, openNote)));
}
