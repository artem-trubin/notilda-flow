import material/material;

import src/ui/ui_fab;

export {
  makeHeader(
    manager : MaterialManager,
    sideNavExpander : DynamicBehaviour<bool>,
  ) -> MAppBar;
}

makeHeader(
  manager : MaterialManager,
  sideNavExpander : DynamicBehaviour<bool>,
) -> MAppBar {
  title = MText("Notilda", []);
  rightElems = [];
  style = [makeFab(manager)];

  toolBar = MToolBar(sideNavExpander, title, rightElems, style);
  MAppBar(toolBar)
};
